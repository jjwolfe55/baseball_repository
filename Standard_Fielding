CREATE TABLE standard_fielding (
player_id VARCHAR(9),
player_name VARCHAR(45),
team_id VARCHAR(3),
season_id SMALLINT,
d_pos VARCHAR(2),
d_g TINYINT,
d_gs TINYINT,
d_pos TINYINT,
d_inn TINYINT,
d_po TINYINT,
d_a TINYINT, 
d_e TINYINT, 
d_dp TINYINT,
d_tp TINYINT,
d_pb TINYINT,        
d_wp TINYINT,          
d_sb TINYINT,            
d_cs TINYINT,             
PRIMARY KEY (game_id,player_id,team_id,d_pos)
);
