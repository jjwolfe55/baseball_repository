CREATE TABLE lines_pitching (
game_id VARCHAR(15),
player_id VARCHAR(9),
team_id VARCHAR(3),
p_sequence TINYINT,
integrity CHAR(8),
p_ipouts TINYINT,
p_noout TINYINT,
p_bf TINYINT,
p_h TINYINT,
p_d TINYINT,
p_t TINYINT,
p_hr TINYINT,
p_r TINYINT,
p_er TINYINT,
p_bb TINYINT,
p_ibb TINYINT,
p_so TINYINT,
p_hbp TINYINT,
p_wp TINYINT,
p_bk TINYINT,
p_sh TINYINT,
p_sf TINYINT,
p_sb TINYINT,
p_cs TINYINT,
p_pb TINYINT,
p_wins TINYINT,
p_losses TINYINT,
p_saves TINYINT,
p_gs TINYINT,
p_gf TINYINT,
p_cg TINYINT,
game_date INT,
game_count TINYINT,
ballpark VARCHAR(7),
opponent VARCHAR(3),
win_loss CHAR(2),
game_type VARCHAR(15),
box_score CHAR(2),
PRIMARY KEY (game_id,player_id,team_id)
);

