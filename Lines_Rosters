CREATE TABLE lines_rosters (
player_id VARCHAR(9),
last_name VARCHAR(20),
first_name VARCHAR(20),
bat CHAR(1),
throw CHAR(1),
team_id VARCHAR(3),
primary_pos VARCHAR(3),
PRIMARY KEY (player_id,team_id)
);

