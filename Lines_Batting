CREATE TABLE lines_batting (
game_id VARCHAR(15),
player_id VARCHAR(9),
team_id VARCHAR(3),
b_lineup TINYINT,
b_sequence TINYINT,
integrity CHAR(8),
b_pa TINYINT,
b_ab TINYINT,
b_r TINYINT,
b_h TINYINT,
b_d TINYINT,
b_t TINYINT,
b_hr TINYINT,
b_rbi TINYINT,
b_sh TINYINT,
b_sf TINYINT,
b_hbp TINYINT,
b_bb TINYINT,
b_ibb TINYINT,
b_so TINYINT,
b_sb TINYINT,
b_cs TINYINT,
b_gdp TINYINT,
b_ci TINYINT,
b_roe TINYINT,
b_dh TINYINT,
b_ph TINYINT,
b_pr TINYINT,
game_date INT,
game_count TINYINT,
ballpark VARCHAR(7),
opponent VARCHAR(3),
win_loss CHAR(2),
game_type VARCHAR(15),
box_score CHAR(2),
PRIMARY KEY (game_id,player_id,team_id)
);

