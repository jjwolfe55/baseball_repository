CREATE TABLE lookup_fielding_positions (
d_pos TINYINT,
d_pos_id VARCHAR(2),
d_pos_name CHAR(15),
PRIMARY KEY (d_pos)
);

INSERT INTO lookup_fielding_positions (
d_pos,
d_pos_id,
d_pos_name)
VALUES 
(1,'P','Pitcher'),
(2,'C','Catcher'),
(3,'1B','First Base'),
(4,'2B','Second Base'),
(5,'3B','Third Base'),
(6,'SS','Shortstop'),
(7,'LF','Left Field'),
(8,'CF','Center Field'),
(9,'RF','Right Field')
;
