CREATE TABLE standard_pitching (
player_id VARCHAR(9),
player_name VARCHAR(45),
team_id VARCHAR(3),
season_id SMALLINT,
p_g SMALLINT,
p_gs SMALLINT,
p_bf SMALLINT,
p_h SMALLINT,
p_s SMALLINT,
p_d SMALLINT,
p_t SMALLINT,
p_hr SMALLINT,
p_r SMALLINT,
p_er SMALLINT,
p_bb SMALLINT,
p_ibb SMALLINT,
p_so SMALLINT,
p_hbp SMALLINT,
p_sh SMALLINT,
p_sf SMALLINT,
p_sb SMALLINT,
p_cs SMALLINT,
p_bk SMALLINT,
p_wp SMALLINT,
p_pb SMALLINT,
p_wins SMALLINT,
p_losses SMALLINT,
p_saves SMALLINT,
PRIMARY KEY (player_id,team_id,season_id)
);
