CREATE TABLE lines_teams (
game_id VARCHAR(15), 
v_team_id VARCHAR(3),
h_team_id VARCHAR(3),
ballpark VARCHAR(7)
game_date INT,
game_count TINYINT,
start_time VARCHAR(10)
day_time CHAR(8),
planned_inn TINYINT,
tiebreaker TINYINT,
use_dh CHAR(5),
h_team_bf CHAR(5),
game_time TINYINT,
attendance INT,
field_condition CHAR(15),
precipitation CHAR(15),
sky_condition CHAR(15),
temp SMALLINT,
wind_direction CHAR(15),
wind_speed TINYINT,
official_scorer VARCHAR(9),
forfeit CHAR(1),
ump_hp VARCHAR(9),
ump1b VARCHAR(9),          
ump2b VARCHAR(9),           
ump3b VARCHAR(9),           
umplf VARCHAR(9),           
umprf VARCHAR(9),
wp VARCHAR(9),
lp VARCHAR(9),
save VARCHAR(9),
game_type VARCHAR(15),
v_team_runs TINYINT,
h_team_runs TINYINT,
winning_team VARCHAR(3),
losing_team VARCHAR(3),
batteries CHAR(1),
lineups CHAR(1),
batteries CHAR(7),
box_score CHAR(1),
play_by_play CHAR(1),
season_id SMALLINT,
PRIMARY KEY (game_id)
);
